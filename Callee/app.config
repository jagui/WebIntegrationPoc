<?xml version="1.0"?>
<configuration>
  <startup>
    
  <supportedRuntime version="v2.0.50727"/></startup>
  <system.serviceModel>
    <services>
      <service name="WcfService.Service" behaviorConfiguration="httpGetEnabled">
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:2996/Service.svc" />
          </baseAddresses>
        </host>
       <endpoint address="" behaviorConfiguration="WcfService.ServiceAspNetAjaxBehavior" binding="webHttpBinding" contract="WcfService.Service"/>
      </service>
    </services>
    <behaviors>
      <endpointBehaviors>
        <behavior name="WcfService.ServiceAspNetAjaxBehavior">
          <enableWebScript/>
        </behavior>
      </endpointBehaviors>
      <serviceBehaviors>
        <behavior name="httpGetEnabled">
          <serviceMetadata httpGetEnabled="true"/>
          <serviceDebug includeExceptionDetailInFaults="true"/>
        </behavior>
      </serviceBehaviors>
    </behaviors>
    <serviceHostingEnvironment aspNetCompatibilityEnabled="true"/>
  </system.serviceModel>
</configuration>
