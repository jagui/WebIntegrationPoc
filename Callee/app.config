<?xml version="1.0"?>
<configuration>

  <startup>
    <supportedRuntime version="v2.0.50727"/>
  </startup>

  <system.serviceModel>

    <services>
      <service name="WcfService.Service">
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:2996/Service.svc"/>
          </baseAddresses>
        </host>
        <endpoint address=""
                          binding="customBinding"
                          bindingConfiguration="jsonpBinding"
                          behaviorConfiguration="webHttpBehavior"
                          contract="WcfService.IScanService" />
      </service>
    </services>

    <behaviors>
      <endpointBehaviors>
        <behavior name="webHttpBehavior">
          <webHttp />
        </behavior>
      </endpointBehaviors>
    </behaviors>

    <extensions>
      <bindingElementExtensions>
        <add name="jsonpMessageEncoding"
             type="Microsoft.Ajax.Samples.JsonpBindingExtension, Callee, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null"/>
      </bindingElementExtensions>
    </extensions>

    <bindings>
      <customBinding>
        <binding name="jsonpBinding" >
          <jsonpMessageEncoding />
          <httpTransport manualAddressing="true"/>
        </binding>
      </customBinding>
    </bindings>
    
  </system.serviceModel>


